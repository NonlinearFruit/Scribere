<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="anchor" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="scribere.FXMLDocumentController">
   <children>
      <HBox fx:id="header" alignment="CENTER">
         <children>
            <Text fx:id="displayText" strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
               <font>
                  <Font size="25.0" />
               </font></Text>
         </children>
      </HBox>
      <TextArea fx:id="writersDOOM" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
